[Term]
id: DyakDv:0000000
name: Drosophila yakuba life cycle stage
def: "A spatiotemporal region encompassing some part of the life cycle of an organism." [UBERON:0000105]
synonym: "developmental stage" EXACT []
synonym: "stage" NARROW []
xref: UBERON:0000105

[Term]
id: DyakDv:0000001
name: Drosophila yakuba life cycle
namespace: Dyakuba_developmental_stage
def: "Temporal interval that defines Drosophila yakuba life from fertilisation to death." [Bgee:curator "Bgee"]
xref: FBdv:00000000
xref: UBERON:0000104
is_a: DyakDv:0000000 ! Drosophila yakuba life cycle stage

[Term]
id: DyakDv:0000002
name: embryonic stage
namespace: Dyakuba_developmental_stage
def: "The stage of Drosophila yakuba life-cycle from fertilization to hatching." [Bgee:curator "Bgee"]
xref: FBdv:00005289
xref: UBERON:0000068
is_a: DyakDv:0000000 ! Drosophila yakuba life cycle stage
relationship: part_of DyakDv:0000001 ! Drosophila yakuba life cycle

[Term]
id: DyakDv:0000003
name: larval stage
namespace: Dyakuba_developmental_stage
def: "The stage of Drosophila yakuba life-cycle from hatching to the beginning of puparium formation." [Bgee:curator "Bgee"]
